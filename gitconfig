[alias] 
	br = branch
	branch-name = "rev-parse --abbrev-ref HEAD"
	# create branch
	chb = checkout -b
	c = commit
	cam = commit --all -m
	cm = commit -m
	co = checkout
	dc = diff --cached
	# list alias
	la = "!git config -l | grep alias | cut -c 7-"
	# push create remote branch if needed
	up = "!git push -u origin $(git branch-name)"

